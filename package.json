{
  "name": "fcp-mpdp-journey-test-suite",
  "version": "0.0.0",
  "type": "module",
  "description": "",
  "engines": {
    "node": ">=22.13.1"
  },
  "author": "Defra DDTS",
  "contributors": [
    "Rana Salem <rana.salem@defra.gov.uk>",
    "John Watson <john.watson1@defra.gov.uk>",
    "Jayne Elliott <jayne.elliott@defra.gov.uk>"
  ],
  "license": "OGL-UK-3.0",
  "aliases": {
    "~": ".",
    "page-objects": "test/page-objects",
    "components": "test/components"
  },
  "scripts": {
    "clean": "rm -rf allure-results && rm -rf allure-report && mkdir -p allure-results && mkdir -p allure-report",
    "test": "npm run clean && playwright test",
    "test:browserstack": "npm run clean && browserstack-node-sdk playwright test --config=playwright.browserstack.config.js",
    "test:local": "npm run clean && playwright test --config=playwright.local.config.js",
    "test:local:debug": "npm run clean && playwright test --config=playwright.local.config.js --debug",
    "test:local:browserstack": "npm run clean && browserstack-node-sdk playwright test --config=playwright.local.browserstack.config.js --browserstack.config=browserstack.local.yml",
    "git:pre-commit-hook": "npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "report": "allure generate allure-results --single-file --clean -o allure-report",
    "report:open": "allure open allure-report",
    "report:publish": "npm run report; ./bin/publish-tests.sh",
    "zap:start": "/zap/zap.sh -daemon -host 0.0.0.0 -port 8080 -config api.addrs.addr.name=.* -config api.addrs.addr.regex=true -config api.disablekey=true -config anticsrf.tokens.token.name=crumb",
    "docker:test:local": "npm run clean && docker compose run --build --rm -e TEST_SCRIPT=local fcp-mpdp-journey-test-suite",
    "docker:test:local:browserstack": "npm run clean && docker compose run --build --rm -e TEST_SCRIPT=local:browserstack fcp-mpdp-journey-test-suite"
  },
  "dependencies": {
    "@axe-core/playwright": "4.10.2",
    "@playwright/test": "1.55.0",
    "allure-commandline": "2.34.1",
    "allure-playwright": "3.3.2",
    "browserstack-local": "1.5.8",
    "browserstack-node-sdk": "1.42.7",
    "global-agent": "3.0.0",
    "undici": "7.12.0"
  },
  "devDependencies": {
    "eslint": "9.31.0",
    "husky": "9.1.7",
    "neostandard": "0.12.2"
  }
}
